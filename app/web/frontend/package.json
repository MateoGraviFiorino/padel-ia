{
  "name": "padel-ai-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Frontend para la aplicación de análisis de padel con IA",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write src/",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "framer-motion": "^11.5.4",
    "lucide-react": "^0.424.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "react-scroll-parallax": "^3.4.5",
    "swiper": "^11.1.11"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "vite": "^5.3.4"
  },
  "keywords": [
    "padel",
    "analytics",
    "ai",
    "video-analysis",
    "sports",
    "react",
    "fastapi"
  ],
  "author": "PadelAI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/padel-ia.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/padel-ia/issues"
  },
  "homepage": "https://github.com/your-username/padel-ia#readme"
}
